<?xml version="1.0"?>
<thlogic>
    <attr sel="#header" th:replace="header :: header"></attr>
    <attr sel="#footer" th:replace="footer :: footer"></attr>

    <attr sel="#create-member-form" th:action="@{/members/new}" th:method="post">
        <attr sel="#email" th:name="email" />
        <attr sel="#pwd" th:name="pwd" />
        <attr sel="#name" th:name="name" />
        <attr sel="#birth" th:name="birth" />
        <attr sel="#gender" th:name="gender" />
        <attr sel="#phone" th:name="phone" />
    </attr>
</thlogic>